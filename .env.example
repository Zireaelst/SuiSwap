# Ethereum Configuration
NEXT_PUBLIC_ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
NEXT_PUBLIC_ETHEREUM_TESTNET_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
NEXT_PUBLIC_ETHEREUM_CHAIN_ID=1
NEXT_PUBLIC_ETHEREUM_TESTNET_CHAIN_ID=11155111

# Sui Configuration
NEXT_PUBLIC_SUI_RPC_URL=https://fullnode.mainnet.sui.io:443
NEXT_PUBLIC_SUI_TESTNET_RPC_URL=https://fullnode.testnet.sui.io:443
NEXT_PUBLIC_SUI_NETWORK=mainnet

# API Keys
NEXT_PUBLIC_1INCH_API_KEY=your_1inch_api_key_here
NEXT_PUBLIC_COINGECKO_API_KEY=your_coingecko_api_key_here
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Database (Development)
DATABASE_URL="file:./dev.db"

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Contract Addresses - Ethereum Mainnet
NEXT_PUBLIC_ETH_HTLC_CONTRACT=0x...
NEXT_PUBLIC_ETH_LIMIT_ORDER_CONTRACT=0x...

# Contract Addresses - Ethereum Sepolia
NEXT_PUBLIC_ETH_SEPOLIA_HTLC_CONTRACT=0x...
NEXT_PUBLIC_ETH_SEPOLIA_LIMIT_ORDER_CONTRACT=0x...

# Contract Addresses - Sui Mainnet
NEXT_PUBLIC_SUI_HTLC_PACKAGE=0x...
NEXT_PUBLIC_SUI_LIMIT_ORDER_PACKAGE=0x...

# Contract Addresses - Sui Testnet
NEXT_PUBLIC_SUI_TESTNET_HTLC_PACKAGE=0x...
NEXT_PUBLIC_SUI_TESTNET_LIMIT_ORDER_PACKAGE=0x...

# 1inch Limit Order Protocol
NEXT_PUBLIC_1INCH_LIMIT_ORDER_CONTRACT_ETH=0x1111111254EEB25477B68fb85Ed929f73A960582
NEXT_PUBLIC_1INCH_LIMIT_ORDER_CONTRACT_POLYGON=0x1111111254EEB25477B68fb85Ed929f73A960582

# Feature Flags
NEXT_PUBLIC_ENABLE_TWAP=true
NEXT_PUBLIC_ENABLE_CONCENTRATED_LIQUIDITY=true
NEXT_PUBLIC_ENABLE_ARBITRAGE=true
NEXT_PUBLIC_ENABLE_MEV_PROTECTION=true

# Development
NEXT_PUBLIC_ENV=development
NEXT_PUBLIC_DEBUG=true
